{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "sourceMap": true,
    "sourceRoot": "./src/",
    "outDir": "./dist/",
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
